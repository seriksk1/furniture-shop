.container {
  display: flex;
  align-items: center;
  width: fit-content;
}

.checkbox {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 18px;
  height: 18px;
  margin-right: 12px;
  border-radius: 4px;
  border: 2px solid #333333;
  cursor: pointer;

  .icon {
    width: 100%;
    height: 100%;

    path {
      fill: #ffffff;
    }
  }

  &.checked {
    background: #333333;
    border: 1px solid #333333;
  }
}

